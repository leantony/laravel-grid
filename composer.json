{
  "name": "leantony/laravel-grid",
  "description": "A grid view for laravel, inspired by the yii2 grid widget",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "leantony",
    "bootstrap",
    "laravel-grid",
    "laravel",
    "pjax",
    "grid",
    "laravel-5",
    "laravel5"
  ],
  "homepage": "https://github.com/leantony/laravel-grid",
  "authors": [
    {
      "name": "Antony [leantony] Chacha",
      "email": "chachaantony@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/view": "~5.6",
    "illuminate/support": "~5.6",
    "illuminate/console": "~5.6",
    "illuminate/contracts": "~5.6",
    "illuminate/http": "~5.6",
    "illuminate/database": "~5.6",
    "illuminate/pagination": "~5.6"
  },
  "suggest": {
    "avengers/bootforms": "For rapid bootstrap form generation",
    "maatwebsite/excel": "For exporting grid data as excel (~3.0)",
    "spatie/laravel-pjax": "For pjax support on the server side"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "classmap": [
      "tests/factories"
    ],
    "psr-4": {
      "Leantony\\Grid\\": "src/",
      "Leantony\\Grid\\Buttons\\": "src/buttons",
      "Leantony\\Grid\\Columns\\": "src/columns",
      "Leantony\\Grid\\Commands\\": "src/commands",
      "Leantony\\Grid\\Filters\\": "src/filters",
      "Leantony\\Grid\\Providers\\": "src/providers",
      "Leantony\\Grid\\Routing\\": "src/routing",
      "Leantony\\Grid\\Test\\Models\\": "tests/models"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Leantony\\Grid\\Providers\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
